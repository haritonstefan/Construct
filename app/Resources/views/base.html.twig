<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
    <title>Construct </title>
    <meta name="description" content=""/>
    <meta name="keywords" content=""/>

    {% stylesheets
    'bundles/app/css/style.css'
    'bundles/app/css/flexslider.css'
    'bundles/app/css/prettyPhoto.css'
    'bundles/app/css/bootstrap.min.css'
    'bundles/app/css/modern-business.css'
    filter="cssrewrite" %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}


    {% javascripts output='js/compiled/vendor.js'
    'bundles/app/js/jquery.min.js'
    'bundles/app/js/jquery.easing.1.3.js'
    'bundles/app/js/jquery-ui-1.8.16.custom.min.js'
    'bundles/app/js/all-in-one-min.js'
    'bundles/app/js/setup.js'
    'bundles/app/js/main.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

</head>

{% block header %}
<header class="header_bg clearfix">
    <div class="container clearfix">

        <!-- Logo -->
        {% image '@AppBundle/Resources/public/images/logo.jpg' %}
        <div class="logo">
            {#<a href="{{ path('index') }}"> <img src="{{ asset_url }}"> </a>#}
        </div>
        {% endimage %}
        <!-- /Logo -->

        <!-- Master Nav -->
        <nav class="main-menu">
            <ul>
                <li><a href="{{ path('index') }}">Home</a></li>
                <li><a href="{{ path('product') }}">Products</a></li>
                <li><a href="{{ path('service') }}">Services</a></li>
                <li><a href="{{ path('about') }}">About</a></li>
                <li><a href="{{ path('contact') }}">Contact</a></li>
            </ul>
        </nav>
        <!-- /Master Nav -->
    </div>
</header>
{% endblock %}

{% block body %}
{% endblock %}


<!-- footer -->
<footer class="footer_bg_bottom clearfix">
    <div class="footer_bottom container">
        <div class="col_2_3">

            <div class="menu">
                <ul>
                    <li><a href="{{ path('index') }}">Home</a></li>
                    <li><a href="{{ path('product') }}">Products</a></li>
                    <li><a href="{{ path('service') }}">Services</a></li>
                    <li><a href="{{ path('about') }}">About</a></li>
                    <li><a href="{{ path('contact') }}">Contact</a></li>
                </ul>
            </div>


            <div class="clear padding20"></div>


            <p>
                &copy; Some Rights Reserved. &nbsp; <a href="#">Legal Notices</a>
            </p>

        </div>

        <div class="clear padding20"></div>
    </div>
</footer>
<!-- /footer -->